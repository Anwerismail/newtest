{
  "name": "evolyte-backend",
  "version": "1.0.0",
  "description": "Evolyte Backend API",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "seed": "node src/database/seeds/admin.seed.js",
    "seed:templates": "node src/database/seeds/templates.seed.js",
    "seed:projects": "node src/database/seeds/projects.seed.js",
    "test": "NODE_ENV=test jest --verbose --runInBand",
    "test:watch": "NODE_ENV=test jest --watch --verbose",
    "test:coverage": "NODE_ENV=test jest --coverage --verbose",
    "test:unit": "NODE_ENV=test jest --testPathPattern=tests/unit --verbose",
    "test:integration": "NODE_ENV=test jest --testPathPattern=tests/integration --verbose --runInBand"
  },
  "keywords": ["Evolyte", "api", "cms"],
  "author": "Anwer Ismail",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "express-validator": "^7.0.1",
    "redis": "^4.6.11",
    "cloudinary": "^1.41.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "stripe": "^14.9.0",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "@sentry/node": "^7.91.0",
    "@sentry/profiling-node": "^1.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.11",
    "mongodb-memory-server": "^9.1.3"
  }
}